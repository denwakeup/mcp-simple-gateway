{
  "proxyServer": {
    "options": {
      "logLevel": "debug",
      "logPretty": true,
      "authTokens": ["proxy-auth-token"]
    }
  },
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=Europe/Moscow"],
      "proxyOptions": {
        "authTokens": ["server-specific-proxy-auth-token"]
      }
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/app/files"]
    }
  }
}
